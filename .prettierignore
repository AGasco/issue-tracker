# Ignore node_modules
node_modules/

# Ignore build output directories
dist/
build/
out/

# Ignore environment variable files
.env
.env.local
.env.*.local

# Ignore logs
*.log
logs/
*.tlog

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore npm/yarn/pnpm lock files
package-lock.json
yarn.lock
pnpm-lock.yaml

# Ignore lock files for other package managers
*.lock

# Ignore coverage directories
coverage/

# Ignore cache directories
.cache/
.vscode/
.vs/
.idea/

# Ignore temporary files
*.tmp
*.temp

# Ignore images and media files
*.png
*.jpg
*.jpeg
*.gif
*.svg
*.ico
*.webp
*.mp4
*.mp3
*.wav
*.ogg
*.mov

# Ignore compressed files
*.zip
*.rar
